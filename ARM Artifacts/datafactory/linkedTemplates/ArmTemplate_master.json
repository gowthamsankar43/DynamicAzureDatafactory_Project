{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "adfcicddevv"
        },
        "ETL Notebook_accessToken": {
            "type": "secureString",
            "metadata": "Secure string for 'accessToken' of 'ETL Notebook'"
        },
        "bronzelayer_accountKey": {
            "type": "secureString",
            "metadata": "Secure string for 'accountKey' of 'bronzelayer'"
        },
        "RestAPI Service_properties_typeProperties_url": {
            "type": "string",
            "defaultValue": "http://api.wmata.com/"
        },
        "bronzelayer_properties_typeProperties_url": {
            "type": "string",
            "defaultValue": "https://adfcicddatalakedev.dfs.core.windows.net/"
        },
        "containerUri": {
            "type": "string",
            "metadata": "URI to storage account containing linked ARM templates"
        },
        "containerSasToken": {
            "type": "string",
            "metadata": "Sas token to storage account containing linked ARM templates"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "adfcicddevv_ArmTemplate_0",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2024-03-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "factoryName": {
                        "value": "[parameters('factoryName')]"
                    },
                    "ETL Notebook_accessToken": {
                        "value": "[parameters('ETL Notebook_accessToken')]"
                    },
                    "bronzelayer_accountKey": {
                        "value": "[parameters('bronzelayer_accountKey')]"
                    },
                    "RestAPI Service_properties_typeProperties_url": {
                        "value": "[parameters('RestAPI Service_properties_typeProperties_url')]"
                    },
                    "bronzelayer_properties_typeProperties_url": {
                        "value": "[parameters('bronzelayer_properties_typeProperties_url')]"
                    }
                }
            },
            "dependsOn": []
        }
    ]
}